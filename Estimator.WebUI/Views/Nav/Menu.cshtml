@model IEnumerable<string>


@foreach (var link in Model)
{
    if (link == "Edit")
    {
            @Html.ActionLink("Edit_Users", "UserList", "User", null, new { @class = "btn btn-block btn-default btn-lg" })
    }
    else {
            @Html.RouteLink(link, new { controller = "Nav", action = link }, new { @class = "btn btn-block btn-default btn-lg" })
    }
}







